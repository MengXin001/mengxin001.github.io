#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/valaxy@0.17.2_@types+markdown-it@13.0.7_@unhead+shared@1.8.10_postcss@8.4.33_typescript@5.3.3_unhead@1.8.10/node_modules/valaxy/bin/node_modules:/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/valaxy@0.17.2_@types+markdown-it@13.0.7_@unhead+shared@1.8.10_postcss@8.4.33_typescript@5.3.3_unhead@1.8.10/node_modules/valaxy/node_modules:/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/valaxy@0.17.2_@types+markdown-it@13.0.7_@unhead+shared@1.8.10_postcss@8.4.33_typescript@5.3.3_unhead@1.8.10/node_modules:/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/valaxy@0.17.2_@types+markdown-it@13.0.7_@unhead+shared@1.8.10_postcss@8.4.33_typescript@5.3.3_unhead@1.8.10/node_modules/valaxy/bin/node_modules:/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/valaxy@0.17.2_@types+markdown-it@13.0.7_@unhead+shared@1.8.10_postcss@8.4.33_typescript@5.3.3_unhead@1.8.10/node_modules/valaxy/node_modules:/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/valaxy@0.17.2_@types+markdown-it@13.0.7_@unhead+shared@1.8.10_postcss@8.4.33_typescript@5.3.3_unhead@1.8.10/node_modules:/mnt/c/Users/Lenovo/Desktop/valaxy-theme-sakura-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../valaxy/bin/valaxy.cjs" "$@"
else
  exec node  "$basedir/../valaxy/bin/valaxy.cjs" "$@"
fi
