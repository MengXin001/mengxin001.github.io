#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules/vite-ssg/bin/node_modules:/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules/vite-ssg/node_modules:/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules:/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules/vite-ssg/bin/node_modules:/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules/vite-ssg/node_modules:/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules:/mnt/c/Users/Lenovo/Desktop/mengxin001.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules/vite-ssg/bin/vite-ssg.js" "$@"
else
  exec node  "$basedir/../../../../../vite-ssg@0.23.6_critters@0.0.20_vite@5.0.12_vue-router@4.2.5_vue@3.4.15/node_modules/vite-ssg/bin/vite-ssg.js" "$@"
fi
